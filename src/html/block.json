{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "actappblk/html",
	"title": "ActApp HTML",
	"category": "actappblk",
	"description": "Add custom HTML code and preview it as you edit.",
	"keywords": [ "embed" ],
	"textdomain": "default",
	"attributes": {
		"content": {
			"type": "string",
			"source": "html"
		}
	},
	"supports": {
		"customClassName": false,
		"className": false,
		"html": false
	},
	"editorStyle": "wp-block-html-editor"
}
